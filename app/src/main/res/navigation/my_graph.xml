<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_graph"
    app:startDestination="@id/giveFragment">

    <fragment
        android:id="@+id/giveFragment"
        android:name="com.example.denemeapp.view.GiveFragment"
        android:label="fragment_give"
        tools:layout="@layout/fragment_give" >
        <action
            android:id="@+id/action_giveFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.denemeapp.view.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>